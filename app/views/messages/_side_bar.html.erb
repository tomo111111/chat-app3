<div class="side-top">
  <div class="user-name">
    <%= link_to current_user.name, edit_user_path(current_user),class:"user-name" %>
  </div>
  <%= link_to "チャットを作成する",new_room_path,class:"new-chat" %>
</div>
<div class="rooms">
  <% current_user.rooms.each do|room| %>
    <div class="room-name">
      <%= link_to room.name,room_messages_path(room) %>
    </div>
  <%end%>
</div>